{
  "name": "Notes-Bot",
  "version": "1.0.3",
  "displayName": "Notes",
  "description": "A bot that can be used to keep notes.",
  "url": "https://git.soma.salesforce.com/Refocus-Bots/Notes-Bot",
  "scripts": {
    "start-ui": "webpack-dev-server --open",
    "start": "npm run build-ui && node index.js",
    "build-ui": "webpack --config webpack.config.js",
    "lint": "eslint --ext=js --ext=jsx .",
    "postinstall": "npm run build-ui",
    "test": "./node_modules/.bin/mocha --reporter spec",
    "test-dev": "nodemon --ext js --watch ./ --exec 'mocha ./test'"
  },
  "dependencies": {
    "@salesforce/refocus-bdk": "^1.6.9",
    "dompurify": "^1.0.8",
    "express": "4.10.2",
    "jsforce": "^1.9.1",
    "lodash": "^4.17.11",
    "mocha": "^3.5.3",
    "moment": "^2.18.1",
    "prop-types": "^15.6.2",
    "react": "^15.6.1",
    "react-contenteditable": "^2.1.6",
    "react-dom": "^15.6.1",
    "react-html-parser": "^2.0.2",
    "serialize-javascript": "^4.0.0",
    "socket.io": "1.2.0",
    "socket.io-client": "^2.0.3",
    "superagent": "3.6.0"
  },
  "babel": {
    "presets": [
      "env",
      "react"
    ]
  },
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-eslint": "^8.2.6",
    "babel-loader": "^7.1.5",
    "babel-preset-env": "^1.5.2",
    "babel-preset-react": "^6.24.1",
    "chai": "4.1.2",
    "css-loader": "^0.28.10",
    "eslint": "^4.18.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.11.1",
    "html-webpack-plugin": "^2.28.0",
    "sinon": "3.2.1",
    "style-loader": "^0.18.2",
    "url-loader": "^0.5.9",
    "webpack": "^2.6.1",
    "webpack-dev-server": "^2.11.3",
    "zip-webpack-plugin": "2.0.0"
  },
  "metadata": {
    "actions": [
      {
        "name": "actionName",
        "parameters": [
          {
            "name": "parameterName",
            "type": "String"
          }
        ]
      }
    ],
    "data": [
      {
        "name": "dataName",
        "type": "String"
      }
    ],
    "settings": [
      {
        "key": "dataName",
        "helpText": "Describe the information needed in this setting"
      }
    ]
  }
}
